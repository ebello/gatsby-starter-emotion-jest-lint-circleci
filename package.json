{
  "name": "gatsby-starter-emotion-jest-lint-circleci",
  "private": true,
  "description": "A simplified starter for Gatsby that includes linting, testing, emotion for CSS-in-JS, and CircleCI support.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:updatesnapshots": "yarn test -- --updateSnapshot",
    "lint:es": "eslint --ignore-path .gitignore \"**/*.{js,jsx}\"",
    "lint:css": "stylelint --ignore-path .gitignore \"**/*.{js,jsx}\"",
    "lint": "yarn lint:es && yarn lint:css"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "gatsby": "^2.18.16",
    "gatsby-plugin-emotion": "^4.1.18",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.23",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.26",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.27",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ebello/gatsby-starter-emotion-jest-lint-circleci"
  }
}
