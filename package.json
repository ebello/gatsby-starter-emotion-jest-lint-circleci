{
  "name": "gatsby-starter-emotion-jest-lint-circleci",
  "private": true,
  "description": "A simplified starter for Gatsby that includes linting, testing, emotion for CSS-in-JS, and CircleCI support.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn develop",
    "serve": "yarn build && gatsby serve -p=8000",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:updatesnapshots": "yarn test -- --updateSnapshot",
    "test:coverage": "jest --collectCoverage",
    "lint:es": "eslint --ignore-path .gitignore \"**/*.{js,jsx}\"",
    "lint:css": "stylelint --ignore-path .gitignore \"**/*.{js,jsx}\"",
    "lint": "yarn lint:es && yarn lint:css"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "gatsby": "^2.20.12",
    "gatsby-plugin-emotion": "^4.1.22",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "babel-jest": "^25.3.0",
    "babel-preset-gatsby": "^0.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "jest-emotion": "^10.0.32",
    "stylelint": "^13.3.2",
    "stylelint-config-standard": "^20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ebello/gatsby-starter-emotion-jest-lint-circleci"
  }
}
